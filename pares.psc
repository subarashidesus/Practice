Proceso pares
	Definir numero1, numero2 Como Real;;
	
	Escribir "Ingresar el primer numero";
	Leer numero1;
	Escribir "Ingresar el segundo numero";
	Leer numero2;
	
	Si numero1 % 2 = 0 O numero2 % 2 = 0 Entonces
		Escribir "Al menos uno de es par";
	SiNo
		Escribir "Ninguno es un numero par";
	FinSi
FinProceso
